{
  "name": "Caesarea+",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Find and highlight specific Nazis",
  "homepage_url": "https://github.com/Nabirius/Caesarea",
  "icons": {
    "16": "icons/icon16.png",
  },
  "permissions": [
    "https://app.everlaw.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://app.everlaw.com/*"],
      "js": ["src/inject/inject.js"]
    }
  ]
}
